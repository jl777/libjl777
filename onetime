./configure
make
cp .libs/libuv.a ../libs
cp .libs/libuv.so ../libs
cd ..
echo "expanding nacl"
bzip2 -dc < nacl-20090405.tar.bz2 | tar -xf -;
cd nacl-20090405;
echo "compiling nacl, this will take some time"
./do
cd ..
echo "randombytes.o and libnacl.a are in the build directory of nacl-20090405"
 
